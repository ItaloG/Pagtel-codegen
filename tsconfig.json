{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["ES2021"],
    "module": "commonjs",
    "outDir": "./dist",
    "removeComments": true,
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true
  },
  "include": [
    "src",
  ],
  "exclude": []
}
